{
	"$schema": "https://typedoc.org/schema.json",
	"entryPointStrategy": "packages",
	"entryPoints": ["../packages/crelte"],
	"out": "./types",
	"docsRoot": "./docs",
	"skipErrorChecking": true,
	"plugin": ["typedoc-plugin-markdown", "typedoc-vitepress-theme"],
	"readme": "none",
	"indexFormat": "table",
	"useCodeBlocks": true,
	"disableSources": true
}
