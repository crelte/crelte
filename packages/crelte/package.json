{
	"name": "crelte",
	"version": "0.5.8",
	"author": "Crelte <support@crelte.com>",
	"type": "module",
	"scripts": {
		"check": "svelte-check",
		"check-fmt": "prettier -c .",
		"fmt": "prettier -w .",
		"lint": "eslint ./src",
		"test": "vitest",
		"build": "svelte-package -i ./src",
		"watch": "svelte-package -i ./src -w"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/crelte/crelte"
	},
	"files": [
		"dist",
		"src",
		"./types.d.ts"
	],
	"module": "dist/index.js",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		},
		"./types": {
			"types": "./types.d.ts"
		},
		"./client": {
			"types": "./dist/init/client.d.ts",
			"default": "./dist/init/client.js"
		},
		"./server": {
			"types": "./dist/server/index.d.ts",
			"default": "./dist/server/index.js"
		},
		"./vite": {
			"types": "./dist/vite/index.d.ts",
			"default": "./dist/vite/index.js"
		},
		"./node": {
			"types": "./dist/node/index.d.ts",
			"default": "./dist/node/index.js"
		},
		"./queries": {
			"types": "./dist/queries/index.d.ts",
			"default": "./dist/queries/index.js"
		},
		"./routing": {
			"types": "./dist/routing/index.d.ts",
			"default": "./dist/routing/index.js"
		},
		"./ssr": {
			"types": "./dist/ssr/index.d.ts",
			"default": "./dist/ssr/index.js"
		},
		"./loadData": {
			"types": "./dist/loadData/index.d.ts",
			"default": "./dist/loadData/index.js"
		},
		"./plugins": {
			"types": "./dist/plugins/index.d.ts",
			"default": "./dist/plugins/index.js"
		},
		"./cookies": {
			"types": "./dist/cookies/index.d.ts",
			"default": "./dist/cookies/index.js"
		},
		"./blocks": {
			"types": "./dist/blocks/index.d.ts",
			"default": "./dist/blocks/index.js"
		},
		"./static": {
			"types": "./dist/static/index.d.ts",
			"default": "./dist/static/index.js"
		},
		"./std": {
			"types": "./dist/std/index.d.ts",
			"default": "./dist/std/index.js"
		},
		"./std/sync": {
			"types": "./dist/std/sync/index.d.ts",
			"default": "./dist/std/sync/index.js"
		},
		"./std/stores": {
			"types": "./dist/std/stores/index.d.ts",
			"default": "./dist/std/stores/index.js"
		},
		"./std/intl": {
			"types": "./dist/std/intl/index.d.ts",
			"default": "./dist/std/intl/index.js"
		},
		"./std/rand": {
			"types": "./dist/std/rand/index.d.ts",
			"default": "./dist/std/rand/index.js"
		}
	},
	"dependencies": {
		"magic-string": "^0.30.17",
		"trouter": "^4.0.0"
	},
	"peerDependencies": {
		"svelte": "^4.0.0 || ^5.39.1",
		"vite": "^5.0.0 || ^6.0.0"
	},
	"devDependencies": {
		"@sveltejs/package": "^2.3.1",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"svelte-check": "^4.1.4",
		"typescript-svelte-plugin": "^0.3.45",
		"vitest": "^3.0.0",
		"@types/node": "^22.13.10"
	}
}
